<!-- -*- mode: XML -*- -->
<launch>
  <!-- Change car name to match the name of the car, e.g. car1 -->
  <arg name="car_name" default="car" />

  <group ns="$(arg car_name)">

    <group ns="vesc">
         <include file="$(find vesc_main)/launch/vesc_main.launch"/>
             <!-- <arg name="racecar_version" value="$(arg racecar_version)" />
         </include> -->
    </group>

    <group ns="teleop">
        <include file="$(find mushr_base)/launch/includes/joy_teleop.launch">
            <arg name="car_name" value="/$(arg car_name)" />
        </include>
    </group>

    <group ns="mux">
        <include file="$(find ackermann_cmd_mux)/launch/mux.launch" />
    </group>

    <!-- <include file="$(find mushr_base)/launch/includes/racecar_state.launch">
        <arg name="tf_prefix" value="" />
    </include>
    
    <include file="$(find patrol_description)/launch/view_model.launch"/> -->

    <node name="serial_node" pkg="rosserial_python" type="serial_node.py">
      <param name="port" value="/dev/mcu/teensy"/>
      <param name="baud" value="115200"/>
    </node>

    <include file="$(find sick_scan_xd)/launch/sick_lms_1xx.launch">
        <arg name="hostname" value="169.254.100.189" />
        <arg name="frame_id" value="laser_lms_laser" />
    </include>

    <!-- <include file="$(find sick_scan_xd)/launch/sick_tim_5xx.launch">
        <arg name="hostname" value="169.254.201.75" />
        <arg name="frame_id" value="laser_tim" />
    </include> -->

  </group>
</launch>
