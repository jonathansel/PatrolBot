<?xml version="1.0"?>
<launch>

   <master auto="start"/>

    <!--- Run AMCL-->
    <!-- <include file="$(find patrol_navigation)/launch/amcl_move.launch" />  -->

  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find patrol_navigation)/config/costmap_common_params.yaml" command="load" ns="global_costmap" /> 
    <rosparam file="$(find patrol_navigation)/config/costmap_common_params.yaml" command="load" ns="local_costmap" />
    
    <rosparam file="$(find patrol_navigation)/config/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find patrol_navigation)/config/global_costmap_params.yaml" command="load" /> 
    <rosparam file="$(find patrol_navigation)/config/base_local_planner_params.yaml" command="load" />
    
    <!--teb-local-planner-->
    <param name="base_local_planner" value="teb_local_planner/TebLocalPlannerROS" />
    <param name="controller_frequency" value="10.0" />
  </node>

  <node pkg="patrol_navigation" type="cmd_vel_to_ackermann_drive.py" name="cmd_to_ackermann"/>

</launch> 